{
    "name": "mariomka/filament-dashboards",
    "description": "Dashboard with filters for Filament",
    "keywords": [
        "mariomka",
        "laravel",
        "filament-dashboards"
    ],
    "homepage": "https://github.com/mariomka/filament-dashboards",
    "support": {
        "issues": "https://github.com/mariomka/filament-dashboards/issues",
        "source": "https://github.com/mariomka/filament-dashboards"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Juarez",
            "email": "mario@mjp.one",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9.7",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Mariomka\\FilamentDashboards\\": "src/",
            "Mariomka\\FilamentDashboards\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mariomka\\FilamentDashboards\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mariomka\\FilamentDashboards\\FilamentDashboardsServiceProvider"
            ],
            "aliases": {
                "FilamentDashboards": "Mariomka\\FilamentDashboards\\Facades\\FilamentDashboards"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
